<%= simple_form_for(@build) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :start %>
    <%= f.input :end %>
    <%= f.association :season %>
    <%= f.input :type %>
    <%= f.input :notes %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
